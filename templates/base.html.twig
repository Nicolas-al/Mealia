<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
        <link rel="preconnect" href="https://fonts.gstatic.com">
        <link href="https://fonts.googleapis.com/css2?family=Dosis:wght@200;400&display=swap" rel="stylesheet"> 
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/css/all.min.css">

        <title>{% block title %}Bienvenue!{% endblock %}</title>
        {% block stylesheets %}
        <link type="text/css" rel="stylesheet" href="{{ asset('/css/style.css') }}" />
        <link type="text/css" rel="stylesheet" href="{{ asset('/css/backend/products.css') }}" />
        <link type="text/css" rel="stylesheet" href="{{ asset('/css/backend/index.css') }}" />
        <link type="text/css" rel="stylesheet" href="{{ asset('/css/product.css') }}" />
        <link type="text/css" rel="stylesheet" href="{{ asset('/css/backend/stock.css') }}" />
        <link type="text/css" rel="stylesheet" href="{{ asset('/css/backend/nav.css') }}" />


        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
        <script src="//code.jquery.com/jquery-1.12.0.min.js"></script>
        <script src="{{ asset('javascript/product.js') }}"></script>
        <script src="{{ asset('javascript/backend/stock.js') }}"></script>
        <script src="{{ asset('javascript/backend/nav.js') }}"></script>


        {% endblock %}
    </body>
</html>
